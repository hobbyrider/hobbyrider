{
  "name": "payload-cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "generate:importmap": "payload generate:importmap",
    "init-db": "tsx scripts/init-db.ts",
    "create-admin": "tsx scripts/create-admin.ts",
